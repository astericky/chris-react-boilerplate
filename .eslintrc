{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "rules": {
    "semi": 0,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "react/jsx-indent": [2, 2],
    "react/jsx-indent-props": [2, 2]
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
